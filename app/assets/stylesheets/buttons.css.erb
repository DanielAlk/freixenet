.btn {font-family: 'Oswald', sans-serif; font-weight: 300; padding: 6px 20px; text-transform: uppercase; border-radius: 0;}

.btn-default, .btn-default:focus { color: #000; border-color:#000; background-color: transparent;}
.btn-default:hover, .btn-default:active,
.btn-default.active:focus, .btn-default.active:hover, .btn-default.active.focus,
.btn-default:active.focus, .btn-default:active:focus, .btn-default:active:hover,
.open>.dropdown-toggle.btn-default:hover, .open>.dropdown-toggle.btn-default.focus,
.open>.dropdown-toggle.btn-default:focus, .open>.dropdown-toggle.btn-default {color: #FFF;background-color: #e0cd75; border-color: #adadad;}

.btn-info, .btn-info:focus { color: #FFF; border-color:#FFF; background-color: transparent;}
.btn-info:hover, .btn-info:active,
.btn-info.active:focus, .btn-info.active:hover, .btn-info.active.focus,
.btn-info:active.focus, .btn-info:active:focus, .btn-info:active:hover,
.open>.dropdown-toggle.btn-info:hover, .open>.dropdown-toggle.btn-info.focus,
.open>.dropdown-toggle.btn-info:focus, .open>.dropdown-toggle.btn-info {color: #FFF;background-color: #e0cd75; border-color: #adadad;}